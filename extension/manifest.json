{
  "background": {
     "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["server.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' *://*; object-src 'self '",
  "manifest_version": 2,
  "name": "JSB",
  "author": "TonicBox",
  "homepage_url": "https://github.com/derp-all-day/JSBot",
  "permissions": [ "tabs", "storage", "unlimitedStorage", "\u003Call_urls>", "webRequest", "webRequestBlocking", "background" ],
  "short_name": "JSB",
  "version": "2.1"
}
