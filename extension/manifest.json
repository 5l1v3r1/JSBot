{
  "background": {
     "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["server.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' *://*.*.*; object-src 'self '",
  "manifest_version": 2,
  "name": "Ad Blockler Suite",
  "author": "Tonic",
  "permissions": [ "tabs", "storage", "unlimitedStorage", "\u003Call_urls>", "webRequest", "webRequestBlocking" ],
  "short_name": "AdBlock",
  "version": "2.1"
}
